{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true,
    "assets": [
      {
        "include": "services/firebase/service-account/*.json",
        "outDir": "dist"
      },
      {
        "include": "services/vk/*.pem",
        "outDir": "dist"
      }
    ]
  }
}
